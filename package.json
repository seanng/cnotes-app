  "scripts": {
    "dev": "next",
    "build": "next build",
    "format": "prettier --write .",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json -r tsconfig-paths/register pages/api",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "seed": "node private/seed.js",
    "start": "next start",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "prisma format",
      "yarn lint --fix",
      "yarn format",
      "git add"
    ]
